  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="cmc-proposal-att.repeatable-MINandMAXoccurs-constraint-rule-15">
    <sch:rule xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" context="*[ @minOccurs  and  @maxOccurs ]">
      <sch:let name="min" value="@minOccurs cast as xs:integer"/>
      <sch:let name="max" value="if ( normalize-space( @maxOccurs ) eq 'unbounded')                         then -1                         else @maxOccurs cast as xs:integer"/>
      <sch:assert test="$max eq -1  or  $max ge $min">@maxOccurs should be greater than or equal to @minOccurs</sch:assert>
    </sch:rule>
    <sch:rule xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" context="*[ @minOccurs  and  not( @maxOccurs ) ]">
      <sch:assert test="@minOccurs cast as xs:integer lt 2">When @maxOccurs is not specified, @minOccurs must be 0 or 1</sch:assert>
    </sch:rule>
  </pattern>
