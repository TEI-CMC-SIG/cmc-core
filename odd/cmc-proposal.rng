              </group>
            </oneOrMore>
          </group>
        </choice>
        <zeroOrMore>
          <group>
            <ref name="cp_model.divBottom"/>
            <zeroOrMore>
              <ref name="cp_model.global"/>
            </zeroOrMore>
          </group>
        </zeroOrMore>
