      <ref name="cp_att.global.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="cp_att.global.linking.attributes">
    <ref name="cp_att.global.linking.attribute.corresp"/>
    <ref name="cp_att.global.linking.attribute.synch"/>
    <ref name="cp_att.global.linking.attribute.sameAs"/>
    <ref name="cp_att.global.linking.attribute.copyOf"/>
    <ref name="cp_att.global.linking.attribute.next"/>
    <ref name="cp_att.global.linking.attribute.prev"/>
    <ref name="cp_att.global.linking.attribute.exclude"/>
    <ref name="cp_att.global.linking.attribute.select"/>
  </define>
  <define name="cp_att.global.linking.attribute.corresp">
    <optional>
      <attribute name="corresp">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(corresponds) points to elements that correspond to the current element in some way.</a:documentation>
        <list>
          <oneOrMore>
            <data type="anyURI"/>
          </oneOrMore>
        </list>
      </attribute>
    </optional>
