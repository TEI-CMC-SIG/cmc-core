         "            "
         sch:rule [
           context = "tei:objectIdentifier"
           "\x{a}" ~
           "               "
           sch:report [
             test = "not(count(*) gt 0)"
             "An objectIdentifier must contain at minimum a single piece of locating or identifying information."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    cp_att.global.attributes,
    empty
  }

cp_nym =
  ## (canonical name) contains the definition for a canonical name or name component of any kind. [13.3.6. Names and Nyms]
  element nym {
    (cp_idno*, cp_model.entryPart*, cp_model.pLike*, cp_nym*),
    cp_att.global.attributes,
    cp_att.typed.attributes,
    cp_att.sortable.attributes,
    ## points to constituent nyms
    attribute parts {
      list { xsd:anyURI+ }
    }?,
    empty
  }

cp_listNym =
  ## (list of canonical names) contains a list of nyms, that is, standardized names for any thing. [13.3.6. Names and Nyms]
  element listNym {
    (cp_model.headLike*,
     cp_desc*,
     (cp_relation | cp_listRelation)*,
     ((cp_nym | cp_listNym)+, (cp_relation | cp_listRelation)*)+),
    cp_att.CMC.attributes,
    cp_att.global.attributes,
    cp_att.typed.attributes,
    cp_att.declarable.attributes,
    cp_att.sortable.attributes,
    empty
  }
cp_att.duration.attributes =
  cp_att.duration.w3c.attributes, cp_att.duration.iso.attributes
cp_model.global.spoken =
  cp_pause | cp_vocal | cp_kinesic | cp_incident | cp_writing | cp_shift
cp_model.recordingPart =
  cp_model.dateLike
  | cp_media
  | cp_respStmt
  | cp_equipment
  | cp_broadcast
cp_model.divPart.spoken = cp_u | cp_annotationBlock
cp_model.divPart.spoken_alternation = cp_u | cp_annotationBlock
cp_model.divPart.spoken_sequence = cp_u, cp_annotationBlock
cp_model.divPart.spoken_sequenceOptional = cp_u?, cp_annotationBlock?
cp_model.divPart.spoken_sequenceOptionalRepeatable =
  cp_u*, cp_annotationBlock*
cp_model.divPart.spoken_sequenceRepeatable = cp_u+, cp_annotationBlock+

cp_scriptStmt =
  ## (script statement) contains a citation giving details of the script used for a spoken text. [8.2. Documenting the Source of Transcribed Speech 2.2.7. The Source Description 15.3.2. Declarable Elements]
  element scriptStmt {
    (cp_model.pLike+ | cp_model.biblLike),
    cp_att.global.attributes,
    cp_att.declarable.attributes,
    empty
  }

cp_recordingStmt =
  ## (recording statement) describes a set of recordings used as the basis for transcription of a spoken text. [8.2. Documenting the Source of Transcribed Speech 2.2.7. The Source Description]
  element recordingStmt {
    (cp_model.pLike+ | cp_recording+), cp_att.global.attributes, empty
  }

cp_recording =
  ## (recording event) provides details of an audio or video recording event used as the source of a spoken text, either directly or from a public broadcast. [8.2. Documenting the Source of Transcribed Speech 15.3.2. Declarable Elements]
  element recording {
    (cp_model.recordingPart | cp_model.pLike)*,
    cp_att.global.attributes,
    cp_att.declarable.attributes,
    cp_att.duration.attributes,
    cp_att.typed.attribute.subtype,
    ## the kind of recording.
    [ a:defaultValue = "audio" ]
    attribute type {
      ## audio recording
      "audio" | ## audio and video recording
        "video"
    }?,
    empty
  }

cp_equipment =
  ## (equipment) provides technical details of the equipment and media used for an audio or video recording used as the source for a spoken text. [8.2. Documenting the Source of Transcribed Speech 15.3.2. Declarable Elements]
  element equipment {
    cp_model.pLike+,
    cp_att.global.attributes,
    cp_att.declarable.attributes,
    empty
  }

cp_broadcast =
  ## (broadcast) describes a broadcast used as the source of a spoken text. [8.2. Documenting the Source of Transcribed Speech 15.3.2. Declarable Elements]
  element broadcast {
    (cp_model.pLike+ | cp_model.biblLike | cp_recording),
    cp_att.global.attributes,
    cp_att.declarable.attributes,
    empty
  }

cp_transcriptionDesc =
  ## describes the set of transcription conventions used, particularly for spoken material. [8.2. Documenting the Source of Transcribed Speech]
  element transcriptionDesc {
    (cp_model.labelLike | cp_model.ptrLike | cp_model.pLike)*,
    cp_att.global.attributes,
    ## supplies an identifier for the encoding convention, independent of any version number.
    attribute ident { xsd:Name },
    ## supplies a version number for the encoding conventions used, if any.
    attribute version {
      xsd:token {
        pattern = "[\d]+[a-z]*[\d]*(\.[\d]+[a-z]*[\d]*){0,3}"
      }
    }?,
    empty
  }

cp_u =
  ## (utterance) contains a stretch of speech usually preceded and followed by silence or by a change of speaker. [8.3.1. Utterances]
  element u {
    cp_macro.paraContent,
    cp_att.global.attributes,
    cp_att.timed.attributes,
    cp_att.declaring.attributes,
    cp_att.ascribed.directed.attributes,
    cp_att.notated.attributes,
    ## (transition) indicates the nature of the transition between this utterance and the previous one.
    [ a:defaultValue = "smooth" ]
    attribute trans {
      ## this utterance begins without unusual pause or rapidity.
      "smooth" | ## this utterance begins with a markedly shorter pause than normal.
        "latching" | ## this utterance begins before the previous one has finished.
        "overlap" | ## this utterance begins after a noticeable pause.
        "pause"
    }?,
    empty
  }

cp_pause =
  ## (pause) marks a pause either between or within utterances. [8.3.2. Pausing]
  element pause {
    empty,
    cp_att.CMC.attributes,
    cp_att.global.attributes,
    cp_att.timed.attributes,
    cp_att.typed.attributes,
    cp_att.ascribed.directed.attributes,
    empty
  }

cp_vocal =
  ## (vocal) marks any vocalized but not necessarily lexical phenomenon, for example voiced pauses, non-lexical backchannels, etc. [8.3.3. Vocal, Kinesic, Incident]
  element vocal {
    cp_model.descLike*,
    cp_att.CMC.attributes,
    cp_att.global.attributes,
    cp_att.timed.attributes,
    cp_att.ascribed.directed.attributes,
    cp_att.typed.attributes,
    ## indicates whether or not the phenomenon is repeated.
    [ a:defaultValue = "false" ]
    attribute iterated {
      xsd:boolean
      | (
         "unknown" | "inapplicable")
    }?,
    empty
  }

cp_kinesic =
  ## (kinesic) marks any communicative phenomenon, not necessarily vocalized, for example a gesture, frown, etc. [8.3.3. Vocal, Kinesic, Incident]
  element kinesic {
    cp_model.descLike*,
    cp_att.CMC.attributes,
    cp_att.global.attributes,
    cp_att.timed.attributes,
    cp_att.typed.attributes,
    cp_att.ascribed.directed.attributes,
    ## indicates whether or not the phenomenon is repeated.
    [ a:defaultValue = "false" ]
    attribute iterated {
      xsd:boolean
      | (
         "unknown" | "inapplicable")
    }?,
    empty
  }

cp_incident =
  ## (incident) marks any phenomenon or occurrence, not necessarily vocalized or communicative, for example incidental noises or other events affecting communication. [8.3.3. Vocal, Kinesic, Incident]
  element incident {
    cp_model.descLike*,
    cp_att.CMC.attributes,
    cp_att.global.attributes,
    cp_att.timed.attributes,
    cp_att.typed.attributes,
    cp_att.ascribed.attributes,
    empty
  }

cp_writing =
  ## (writing) contains a passage of written text revealed to participants in the course of a spoken text. [8.3.4. Writing]
  element writing {
    cp_macro.paraContent,
    cp_att.CMC.attributes,
    cp_att.global.attributes,
    cp_att.ascribed.directed.attributes,
    cp_att.typed.attributes,
    cp_att.timed.attributes,
    ## indicates whether the writing is revealed all at once or gradually.
    attribute gradual {
      xsd:boolean
      | (
         "unknown" | "inapplicable")
    }?,
    empty
  }

cp_shift =
  ## (shift) marks the point at which some paralinguistic feature of a series of utterances by any one speaker changes. [8.3.6. Shifts]
  element shift {
    empty
    >> sch:pattern [
         id = "cmc-proposal-shift-shiftNew-constraint-assert-16"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:shift"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@new"
             role = "warning"
             "              \x{a}" ~
             "The @new attribute should always be supplied; use the special value\x{a}" ~
             '"normal" to indicate that the feature concerned ceases to be\x{a}' ~
             "remarkable at this point."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    cp_att.CMC.attributes,
    cp_att.global.attributes,
    cp_att.ascribed.attributes,
    ## a paralinguistic feature.
    ## Suggested values include: 1] tempo; 2] loud; 3] pitch; 4] tension; 5] rhythm; 6] voice
    attribute feature {
      ## speed of utterance.
      "tempo" | ## loudness.
        "loud" | ## pitch range.
        "pitch" | ## tension or stress pattern.
        "tension" | ## rhythmic qualities.
        "rhythm" | ## voice quality.
        "voice"
      | xsd:token { pattern = "[^\p{C}\p{Z}]+" }
    }?,
    ## specifies the new state of the paralinguistic feature specified.
    [ a:defaultValue = "normal" ]
    attribute new {
      xsd:token { pattern = "[^\p{C}\p{Z}]+" }
    }?,
    empty
  }

cp_annotationBlock =
  ## groups together various annotations, e.g. for parallel interpretations of a spoken segment. [8.4.6. Analytic Coding]
  element annotationBlock {
    (cp_u | cp_spanGrp | cp_model.global.spoken)*,
    cp_att.ascribed.attributes,
    cp_att.timed.attributes,
    cp_att.global.attributes,
    empty
  }
cp_TEI =
  [
    a:documentation [
      "(TEI document) contains a single TEI-conformant document, combining a single TEI header with one or more members of the model.resource class. Multiple "
      ns1:code [ "<TEI>" ]
      " elements may be combined within a "
      ns1:code [ "<TEI>" ]
      " (or "
      ns1:code [ "<teiCorpus>" ]
      ") element. [4. Default Text Structure 15.1. Varieties of Composite Text]"
    ]
  ]
  element TEI {
    (cp_teiHeader,
     ((cp_model.resource+, cp_TEI*) | cp_TEI+))
    >> sch:ns [ prefix = "tei" uri = "http://www.tei-c.org/ns/1.0" ]
    >> sch:ns [ prefix = "xs" uri = "http://www.w3.org/2001/XMLSchema" ]
    >> sch:ns [
         prefix = "rng"
         uri = "http://relaxng.org/ns/structure/1.0"
       ],
    cp_att.global.attributes,
    cp_att.typed.attributes,
    ## specifies the version number of the TEI Guidelines against which this document is valid.
    attribute version {
      xsd:token { pattern = "[\d]+(\.[\d]+){0,2}" }
    }?,
    empty
  }

cp_text =
  ## (text) contains a single text of any kind, whether unitary or composite, for example a poem or drama, a collection of essays, a novel, a dictionary, or a corpus sample. [4. Default Text Structure 15.1. Varieties of Composite Text]
  element text {
    (cp_model.global*,
     (cp_front, cp_model.global*)?,
     (cp_body | cp_group),
     cp_model.global*,
     (cp_back, cp_model.global*)?),
    cp_att.global.attributes,
    cp_att.declaring.attributes,
    cp_att.typed.attributes,
    cp_att.written.attributes,
    empty
  }

cp_body =
  ## (text body) contains the whole body of a single unitary text, excluding any front or back matter. [4. Default Text Structure]
  element body {
    (cp_model.global*,
     (cp_model.divTop, (cp_model.global | cp_model.divTop)*)?,
     (cp_model.divGenLike, (cp_model.global | cp_model.divGenLike)*)?,
     ((cp_model.divLike, (cp_model.global | cp_model.divGenLike)*)+
      | (cp_model.div1Like, (cp_model.global | cp_model.divGenLike)*)+
      | ((cp_model.common, cp_model.global*)+,
         ((cp_model.divLike, (cp_model.global | cp_model.divGenLike)*)+
          | (cp_model.div1Like,
             (cp_model.global | cp_model.divGenLike)*)+)?)),
     (cp_model.divBottom, cp_model.global*)*),
    cp_att.global.attributes,
    cp_att.declaring.attributes,
    empty
  }

cp_group =
  ## (group) contains the body of a composite text, grouping together a sequence of distinct texts (or groups of such texts) which are regarded as a unit for some purpose, for example the collected works of an author, a sequence of prose essays, etc. [4. Default Text Structure 4.3.1. Grouped Texts 15.1. Varieties of Composite Text]
  element group {
    ((cp_model.divTop | cp_model.global)*,
     ((cp_text | cp_group), (cp_text | cp_group | cp_model.global)*),
     cp_model.divBottom*),
    cp_att.global.attributes,
    cp_att.declaring.attributes,
    cp_att.typed.attributes,
    empty
  }

cp_floatingText =
  ## (floating text) contains a single text of any kind, whether unitary or composite, which interrupts the text containing it at any point and after which the surrounding text resumes. [4.3.2. Floating Texts]
  element floatingText {
    (cp_model.global*,
     (cp_front, cp_model.global*)?,
     (cp_body | cp_group),
     cp_model.global*,
     (cp_back, cp_model.global*)?),
    cp_att.CMC.attributes,
    cp_att.global.attributes,
    cp_att.declaring.attributes,
    cp_att.typed.attributes,
    empty
  }

cp_div =
  ## (text division) contains a subdivision of the front, body, or back of a text. [4.1. Divisions of the Body]
  element div {
    ((cp_model.divTop | cp_model.global)*,
     ((((cp_model.divLike | cp_model.divGenLike), cp_model.global*)+
       | ((cp_model.common, cp_model.global*)+,
          ((cp_model.divLike | cp_model.divGenLike),
           cp_model.global*)*)),
      (cp_model.divBottom, cp_model.global*)*)?)
    >> sch:pattern [
         id =
           "cmc-proposal-div-abstractModel-structure-l-constraint-report-17"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:div"
           "\x{a}" ~
           "               "
           sch:report [
             test = "ancestor::tei:l"
             "\x{a}" ~
             "        Abstract model violation: Lines may not contain higher-level structural elements such as div.\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "cmc-proposal-div-abstractModel-structure-p-constraint-report-18"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:div"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "ancestor::tei:p or ancestor::tei:ab and not(ancestor::tei:floatingText)"
             "\x{a}" ~
             "        Abstract model violation: p and ab may not contain higher-level structural elements such as div.\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    cp_att.global.attributes,
    cp_att.divLike.attributes,
    cp_att.typed.attributes,
    cp_att.declaring.attributes,
    cp_att.written.attributes,
    empty
  }

cp_div1 =
  ## (level-1 text division) contains a first-level subdivision of the front, body, or back of a text. [4.1.2. Numbered Divisions]
  element div1 {
    ((cp_model.divTop | cp_model.global)*,
     ((((cp_model.div2Like | cp_model.divGenLike), cp_model.global*)+
       | ((cp_model.common, cp_model.global*)+,
          ((cp_model.div2Like | cp_model.divGenLike),
           cp_model.global*)*)),
      (cp_model.divBottom, cp_model.global*)*)?),
    cp_att.global.attributes,
    cp_att.divLike.attributes,
    cp_att.typed.attributes,
    cp_att.declaring.attributes,
    empty
  }

cp_div2 =
  ## (level-2 text division) contains a second-level subdivision of the front, body, or back of a text. [4.1.2. Numbered Divisions]
  element div2 {
    ((cp_model.divTop | cp_model.global)*,
     ((((cp_model.div3Like | cp_model.divGenLike), cp_model.global*)+
       | ((cp_model.common, cp_model.global*)+,
          ((cp_model.div3Like | cp_model.divGenLike),
           cp_model.global*)*)),
      (cp_model.divBottom, cp_model.global*)*)?),
    cp_att.global.attributes,
    cp_att.divLike.attributes,
    cp_att.typed.attributes,
    cp_att.declaring.attributes,
    empty
  }

cp_div3 =
  ## (level-3 text division) contains a third-level subdivision of the front, body, or back of a text. [4.1.2. Numbered Divisions]
  element div3 {
    ((cp_model.divTop | cp_model.global)*,
     ((((cp_model.div4Like | cp_model.divGenLike), cp_model.global*)+
       | ((cp_model.common, cp_model.global*)+,
          ((cp_model.div4Like | cp_model.divGenLike),
           cp_model.global*)*)),
      (cp_model.divBottom, cp_model.global*)*)?),
    cp_att.global.attributes,
    cp_att.divLike.attributes,
    cp_att.typed.attributes,
    cp_att.declaring.attributes,
    empty
  }

cp_div4 =
  ## (level-4 text division) contains a fourth-level subdivision of the front, body, or back of a text. [4.1.2. Numbered Divisions]
  element div4 {
    ((cp_model.divTop | cp_model.global)*,
     ((((cp_model.div5Like | cp_model.divGenLike), cp_model.global*)+
       | ((cp_model.common, cp_model.global*)+,
          ((cp_model.div5Like | cp_model.divGenLike),
           cp_model.global*)*)),
      (cp_model.divBottom, cp_model.global*)*)?),
    cp_att.global.attributes,
    cp_att.divLike.attributes,
    cp_att.typed.attributes,
    cp_att.declaring.attributes,
    empty
  }

cp_div5 =
  ## (level-5 text division) contains a fifth-level subdivision of the front, body, or back of a text. [4.1.2. Numbered Divisions]
  element div5 {
    ((cp_model.divTop | cp_model.global)*,
     ((((cp_model.div6Like | cp_model.divGenLike), cp_model.global*)+
       | ((cp_model.common, cp_model.global*)+,
          ((cp_model.div6Like | cp_model.divGenLike),
           cp_model.global*)*)),
      (cp_model.divBottom, cp_model.global*)*)?),
    cp_att.global.attributes,
    cp_att.divLike.attributes,
    cp_att.typed.attributes,
    cp_att.declaring.attributes,
    empty
