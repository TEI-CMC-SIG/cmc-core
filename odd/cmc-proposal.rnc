cp_div6 =
  ## (level-6 text division) contains a sixth-level subdivision of the front, body, or back of a text. [4.1.2. Numbered Divisions]
  element div6 {
    ((cp_model.divTop | cp_model.global)*,
     ((((cp_model.div7Like | cp_model.divGenLike), cp_model.global*)+
       | ((cp_model.common, cp_model.global*)+,
          ((cp_model.div7Like | cp_model.divGenLike),
           cp_model.global*)*)),
      (cp_model.divBottom, cp_model.global*)*)?),
    cp_att.global.attributes,
